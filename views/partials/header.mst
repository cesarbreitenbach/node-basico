        <strong> {{.}} </strong>
    </div>
    {{/success}}
    {{#error}}
    <div class="warning">
        <strong> {{.}} </strong>
    </div>
    {{/error}}
{{#info}}
    <div class="warning">
        <strong> {{.}} </strong>
    </div>
{{/info}}
{{/flashes}}
